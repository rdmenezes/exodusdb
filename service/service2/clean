rm -I -rf */DATAVOL\\*
rm -I -rf DATAVOL\\*

rm -I -f arev/*

rm -I -f nano.save ./*/nano.save
rm -I -f sselect.lst ./*/sselect.lst
rm -I -f eeval.cpp2 ./*/eeval.cpp2
